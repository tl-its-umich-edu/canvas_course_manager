[{
  "name": "Add multiple users to Canvas sections through CSV",
  "id": "users_in_sections",
  "type": "csv",
  "notes":"Servlet supplies course_id, SAA supplies rest",
  "fields": [{
      "name": "user_id",
      "validation": {
        "spaces": false,
        "chars": "num",
        "pattern":"\\d{8,8}",
        "val_message":"Please use an 8 digit number",
        "max": 8,
        "min": 8
      }
    }, {
      "name": "role",
      "validation": {
        "spaces": false,
        "chars": "alpha",
        "choices": ["student", "teacher", "ta", "observer", "designer"],
        "max": 2000,
        "min": 1
      }
    }, {
      "name": "section_id",
      "validation": {
        "spaces": false,
        "chars": "num",
        "max": 300,
        "min": 1
      }
    },
    {
      "name": "status",
      "validation": {
        "spaces": false,
        "chars": "alpha",
        "choices": ["active", "deleted", "completed", " inactive"],
        "max": 2000,
        "min": 1
      }
    }
  ]
}, {
