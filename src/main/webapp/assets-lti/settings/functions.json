[{
  "name": "Add multiple users to course sections through CSV",
  "id": "users_in_sections",
  "url":"addUsersToSections",
  "type": "csv",
  "notes":"Servlet supplies course_id, SAA supplies rest",
  "fields": [{
      "name": "user_id",
      "validation": {
        "spaces": false,
        "chars": "alpha",
        "pattern":"^\\d{8,8}$|.*@.*",
